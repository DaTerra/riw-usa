=EE=EB=ED=E5=ED=E8=FF =EF=EE=EB=E5=E9 (=EF=EE=EC=E5=F7=E5=ED=FB =E7=E2=E5=
=E7=E4=EE=F7=EA=EE=E9)"); SR_focus.focus(); return false; }
  if (!f["field_email"].value.match(/^[\+A-Za-z0-9][\+A-Za-z0-9\._-]*[\+A=
-Za-z0-9_]*@([A-Za-z0-9]+([A-Za-z0-9-]*[A-Za-z0-9]+)*\.)+[A-Za-z]+$/)) { =
