function SR_IsListSelected(el)
{
  for (var i =3D 0; i < el.length; i ++)
    if (el[i].selected ||
      el[i].checked)
      return i;
  return -1;
}
function SR_trim(f)
{
  return f.toString().replace(/^[ ]+/, '').replace(/[ ]+$/, '');
}
function SR_submit(f)
{
  f["field_email"].value =3D SR_trim(f["field_email"].value);
  f["field_name_first"].value =3D SR_trim(f["field_name_first"].value);
  if ((SR_focus =3D f["field_email"]) && f["field_email"].value.replace(/=
^[ ]+/, '').replace(/[ ]+$/, '').length < 1 || (SR_focus =3D f["field_nam=
e_first"]) && f["field_name_first"].value.replace(/^[ ]+/, '').replace(/[=
 ]+$/, '').length < 1) { alert("=D3=EA=E0=E6=E8=F2=E5 =E7=ED=E0=F7=E5=ED=E8=
